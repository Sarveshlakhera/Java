name: Java CI Pull Request

on:
  pull_request:
    branches:
      - main
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
     - uses: actions/checkout@v2
     - uses: actions/setup-java@v1
     - name: Set up JDK 8
        with:
         java-version: '8.0.192'
         java-package: jdk
         architecture: x64
      - name: Build with Maven
        run: mvn clean install
        